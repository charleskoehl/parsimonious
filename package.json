{
  "name": "parsimonious",
  "version": "1.6.1",
  "description": "Utilities for Parse Server cloud code and JS SDK.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/charleskoehl/parsimonious.git"
  },
  "author": {
    "name": "Charles Koehl",
    "email": "charles@etherscapes.com"
  },
  "scripts": {
    "commit": "git-cz",
    "compile": "babel src -d lib",
    "prepublish": "npm run compile; npm run docs",
    "start": "mocha --watch --compilers js:babel-register",
    "test": "mocha --compilers js:babel-register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md --template README.hbs --files ./src/index.js --param-list-format list > README.md"
  },
  "main": "lib/index.js",
  "keywords": [
    "parse",
    "utilities",
    "utils",
    "utility"
  ],
  "engines": {
    "node": ">=5"
  },
  "dependencies": {
    "auto-bind": "^1.1.0",
    "lodash": "^4.17.4",
    "parse": "^1.9.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.4",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.2.0",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
