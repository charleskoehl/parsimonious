{
  "name": "parsimonious",
  "version": "3.5.4",
  "description": "Utilities for Parse Server cloud code and JS SDK",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/charleskoehl/parsimonious.git"
  },
  "author": {
    "name": "Charles Koehl",
    "email": "charles@etherscapes.com"
  },
  "scripts": {
    "compile": "babel src -d lib",
    "prepublishOnly": "npm run compile; npm run docs; git add .; git commit -m 'prepublishOnly'; git push origin master;",
    "test": "jest -c jest-config.json",
    "test-ci": "jest -c jest-config-ci.json",
    "docs": "jsdoc2md --template README.hbs --files ./src/Parsimonious.js --param-list-format table > README.md"
  },
  "main": "lib/node.js",
  "browser": "lib/browser.js",
  "keywords": [
    "parse",
    "utilities",
    "utils",
    "utility"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "auto-bind": "^1.1.0",
    "lodash": "^4.17.4",
    "parse": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "codecov": "^2.3.1",
    "jest": "^21.2.1",
    "jest-diff": "^21.2.1",
    "jsdoc-to-markdown": "^3.0.0",
    "parse-mockdb": "^0.1.25"
  }
}
