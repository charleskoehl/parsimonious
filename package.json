{
  "name": "parsimonious",
  "version": "2.0.2",
  "description": "Utilities for Parse Server cloud code and JS SDK.",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/charleskoehl/parsimonious.git"
  },
  "author": {
    "name": "Charles Koehl",
    "email": "charles@etherscapes.com"
  },
  "scripts": {
    "commit": "git-cz",
    "compile": "babel src -d lib",
    "prepare": "npm run compile; npm run docs; git add .; git commit -m 'prepare'; git push origin master;",
    "start": "mocha --watch --compilers js:babel-register",
    "test": "npm run compile; jest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md --template README.hbs --files ./src/index.js --param-list-format table > README.md"
  },
  "main": "lib/index.js",
  "keywords": [
    "parse",
    "utilities",
    "utils",
    "utility"
  ],
  "engines": {
    "node": ">=5"
  },
  "dependencies": {
    "auto-bind": "^1.1.0",
    "lodash": "^4.17.4",
    "parse": "^1.10.0",
    "parse-shim": "github:charleskoehl/parse-shim"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "commitizen": "^2.9.6",
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "jest": "^21.1.0",
    "jest-diff": "^21.1.0",
    "jsdoc-to-markdown": "^3.0.0",
    "parse-mockdb": "^0.1.25",
    "semantic-release": "^8.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./set-up-jest.js"
  }
}
